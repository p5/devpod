    "@tanstack/react-query": "4.29.19",
    "@tanstack/react-query-devtools": "4.29.19",
    "@tanstack/react-table": "8.9.3",
    "@tauri-apps/api": "1.4.0",
    "dayjs": "1.11.9",
    "framer-motion": "10.12.20",
    "markdown-to-jsx": "7.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.45.1",
